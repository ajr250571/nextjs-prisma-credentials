generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model elitex {
  id      Int      @id @default(autoincrement())
  fecha   DateTime
  turno   Int
  reloj   Int
  fila    Int
  telar   Int
  lec_act Int
  lec_ant Int
  hrs     Int
  rpm     Int
  rend    Decimal

  @@index([fecha])
  @@index([fila])
  @@index([telar])
  @@index([reloj])
}

model rpm {
  id    Int @id @default(autoincrement())
  fila  Int
  telar Int
  rpm   Int
}

model user {
  id        Int      @id @default(autoincrement())
  email     String   @unique()
  username  String   @unique()
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
